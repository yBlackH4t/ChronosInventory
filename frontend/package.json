{
  "name": "frontend",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:backend": "powershell -ExecutionPolicy Bypass -File ..\\\\build_backend.ps1",
    "verify:sidecar": "powershell -ExecutionPolicy Bypass -File .\\\\scripts\\\\verify_sidecar.ps1",
    "build:app": "npm run build:backend && npm run verify:sidecar && tauri build --bundles msi",
    "build:app:msi": "npm run build:backend && npm run verify:sidecar && tauri build --bundles msi",
    "build:all": "npm run build:app",
    "release:build": "npm run build:backend && npm run verify:sidecar && tauri build",
    "release:sign": "powershell -ExecutionPolicy Bypass -File .\\\\scripts\\\\release_sign.ps1",
    "release:publish": "powershell -ExecutionPolicy Bypass -File .\\\\scripts\\\\release_publish.ps1",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mantine/core": "^8.3.14",
    "@mantine/dates": "^8.3.14",
    "@mantine/dropzone": "^8.3.14",
    "@mantine/form": "^8.3.14",
    "@mantine/hooks": "^8.3.14",
    "@mantine/modals": "^8.3.14",
    "@mantine/notifications": "^8.3.14",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-query": "^5.90.20",
    "@tauri-apps/api": "^1.6.0",
    "dayjs": "^1.11.19",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tauri-apps/cli": "^1.6.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
